{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit",
  },

  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.expo/**": true,
    "**/android/**": true,
    "**/ios/**": true,
    "**/.git/**": true,
    "**/.venv/**": true,

  }  
}
